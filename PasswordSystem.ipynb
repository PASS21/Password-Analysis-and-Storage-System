    "# Checks how many times a password has been breached\n",
    "def breachCheck(passwd):\n",
    "    import pwnedpasswords \n",
    "    #passwd=getpass(\"Enter password: \")\n",
    "    #print(f\"This password has been breached times\")  # Using external APIs to test how many times a password has been breached\n",
    "    print(pwnedpasswords.check(passwd))\n",
